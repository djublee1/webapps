<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script>
$(document).ready(function() {
    var options = {
        type: 'remote',
        rowDetail: {
            expand: 'fa fa-caret-down',
            collapse: 'fa fa-caret-right'
        }
    }
    var datatable = $('.my_datatable').KTDatatable(options);

    for (let index = 1; index <= 42; index++) {
        new KTImageInput('kt_image_'+ index);
    }
    

    $("#imgInp").change(function() {
        readURL(this);
    });

    var id = "<%= data.merk_id %>";
    if(id != "" || id != null){
        loadGroupModel(id);
    }

    var group_id = "<%= data.group_model_id %>";
    if(group_id != "" || group_id != null){
        loadModel(group_id);
    }
});

$(document).on("change", ".file_multi_video", function(evt) {
  var $source = $('#video_here');
  $source[0].src = URL.createObjectURL(this.files[0]);
  $source.parent()[0].load();
});

function readURL(input) {
  if (input.files && input.files[0]) {
    var reader = new FileReader();
    
    reader.onload = function(e) {
      $('#blah').attr('src', e.target.result);
    }
    
    reader.readAsDataURL(input.files[0]); // convert to base64 string
  }
}

$(document).on('change', '#merk', function(){
    var id = $(this).val();
    loadGroupModel(id);
    loadModel("")
});

function loadGroupModel(merk_id){
    var group_model_id = "<%= data.group_model_id %>";
    $.getJSON("/groupmodel/json/"+merk_id, function(data){
        var html = "";
        html += "<option value=''> Please choose </option>";
        $.each(data, function(key, value){ 
            if(group_model_id != "" && group_model_id == value.id){
                html += "<option value='"+value.id+"' selected> "+value.name+" </option>";
            }else{
                html += "<option value='"+value.id+"'> "+value.name+" </option>";
            }
        })
        $("#groupmodel").html(html);
    });
}

$(document).on('change', '#groupmodel', function(){
    var id = $(this).val();
    loadModel(id);
});

function loadModel(id){
    if(id == ""){
        var html = "<option value=''> Please choose </option>";
        $("#model").html(html);
        return;
    }
    var model_id = "<%= data.model_id %>";
    $.getJSON("/model/json/"+id, function(data){
        var html = "";
        html += "<option value=''> Please choose </option>";
        $.each(data, function(key, value){ 
            if(model_id != "" && model_id == value.id){
                html += "<option value='"+value.id+"' selected> "+value.name+" </option>";
            }else{
                html += "<option value='"+value.id+"'> "+value.name+" </option>";
            }
            
        })
        $("#model").html(html);
    });
}

$(document).on('change', '#merkPopup', function(){
    var id = $(this).val();
    loadGroupModelPopup(id);
    loadModelPopup("")
});

function loadGroupModelPopup(merk_id){
    $.getJSON("/groupmodel/json/"+merk_id, function(data){
        var html = "";
        html += "<option value=''> Please choose </option>";
        $.each(data, function(key, value){ 
            html += "<option value='"+value.id+"'> "+value.name+" </option>";
        })
        $("#groupmodelPopup").html(html);
    });
}

$(document).on('change', '#groupmodelPopup', function(){
    var id = $(this).val();
    loadModelPopup(id);
});

function loadModelPopup(id){
    if(id == ""){
        var html = "<option value=''> Please choose </option>";
        $("#modelPopup").html(html);
        return;
    }

    $.getJSON("/model/json/"+id, function(data){
        var html = "";
        html += "<option value=''> Please choose </option>";
        $.each(data, function(key, value){ 
            html += "<option value='"+value.id+"'> "+value.name+" </option>";
        })
        $("#modelPopup").html(html);
    });
}

function copyData(){
    var merk = $("#merkPopup").val();
    var groupmodel = $("#groupmodelPopup").val();
    var model = $("#modelPopup").val();

    window.location.href = '/mobil/copy/?merk=' + merk + '&groupmodel=' + groupmodel + '&model=' + model;
}
</script>