                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Copy Data From</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <i aria-hidden="true" class="ki ki-close"></i>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="form-group col-sm-12">
                                        <label>Merk</label>
                                        <select class="form-control form-control-solid" name="merkPopup" id="merkPopup">
                                            <option value="">Please choose</option>
                                            <% for(var i=0; i < merk.length; i++) { %>
                                                <option value="<%= merk[i].id %>"><%= merk[i].name %></option>
                                            <% } %>
                                        </select>
                                    </div>
                                    <div class="form-group col-sm-12">
                                        <label>Group Model</label>
                                        <select class="form-control form-control-solid" name="groupmodelPopup" id="groupmodelPopup">
                                            <option value="">Please choose</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="form-group col-sm-12">
                                        <label>Model</label>
                                        <select class="form-control form-control-solid" name="modelPopup" id="modelPopup">
                                            <option value="">Please choose</option>
                                        </select>
                                    </div>
                                    <div class="form-group col-sm-12">
                                        <label>Tahun</label>
                                        <select class="form-control form-control-solid" name="tahunPopup" id="tahun">
                                            <% for(var i=0; i < tahun.length; i++) { %>
                                                <option value="<%= tahun[i].tahun %>"><%= tahun[i].tahun %></option>
                                            <% } %>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-light-primary font-weight-bold" data-dismiss="modal">Close</button>
                            <button type="button" onclick="copyData()" class="btn btn-primary font-weight-bold">Copy</button>
                        </div>
                    </div>
